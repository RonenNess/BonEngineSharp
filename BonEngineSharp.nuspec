<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
	<id>BonEngineSharp</id>
	<version>1.5.4</version>
	<authors>Ronen Ness</authors>
	<owners>RonenNess</owners>
	<title>BonEngine Sharp</title>
	<summary>BonEngine is a code-based game engine designed to be simple and straightforward, with as little setup as possible. It covers 2d rendering, assets, sound effects, music, input, config files, and more.</summary>
	<licenseUrl>https://en.wikipedia.org/wiki/MIT_License</licenseUrl>
	<projectUrl>https://github.com/RonenNess/BonEngineSharp</projectUrl>
	<iconUrl>https://raw.githubusercontent.com/RonenNess/BonEngineSharp/master/nuget_icon.png</iconUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<releaseNotes>Fixed alpha channels for images without alpha, added round pixels feature, and added some extras.</releaseNotes>
	<description>BonEngine is a code-based game engine designed to be fast, simple and straightforward.</description>
	<tags>game engine,gamedev,graphics,gamedev</tags>
	<dependencies>
	</dependencies>
    <contentFiles> 
      <files include="any/any/**.*" buildAction="None" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <!-- targets and props files -->
    <file src="BonEngineSharp.targets" target="build\BonEngineSharp.targets" />
    <file src="BonEngineSharp.props" target="build\BonEngineSharp.props" />
    <!-- x86 -->
	<file src="BonEngineSharp\bin\x86\Release\netcoreapp3.0\BonEngineSharp.dll" target="build\x86\BonEngineSharp.dll"/>
    <!-- x64 -->
    <file src="BonEngineSharp\bin\x64\Release\netcoreapp3.0\BonEngineSharp.dll" target="build\x64\BonEngineSharp.dll"/>  
    <!-- core dlls -->
    <file src="BonEngineSharp\BoneEngineCore\_x64\*" target="contentFiles\any\any\BoneEngineCore\_x64"/> 
    <file src="BonEngineSharp\BoneEngineCore\_x86\*" target="contentFiles\any\any\BoneEngineCore\_x86"/> 
    <!-- miscs -->
    <file src="LICENSE" target="LICENSE" /> 
  </files>
</package>
